@model CB.BACKOFICEOFICIAL.Clases.Imprimir
@{
    ViewBag.Title = "printf";
    Layout = "~/Views/Shared/_LayoutImprimir.cshtml";
}

<div class="wrapper">
    <!-- Main content -->
    <section class="invoice">
        <!-- title row -->
        <div class="row">
            <div class="col-xs-12">
                <h2 class="page-header">
                    <i class="fa fa-globe"></i> Obelisco, S.A.
                    <small class="pull-right">@ViewBag.fecha</small>
                </h2>
            </div>
            <!-- /.col -->
        </div>
        <!-- info row -->
        <div class="row invoice-info">
            <div class="col-sm-12 invoice-col">
                <b>Datos generales</b><br>
                <br>
                <b>Nombre:</b> @Model.cliente.NombreP<br>
                <b>Apellidos:</b> @Model.cliente.Apellido<br>
                <b>CI:</b> @Model.cliente.CI <br>
                <b>Cod Cliente:</b> @Model.cliente.CodCliente <br>
                <b>Teelfono:</b> @Model.cliente.Telefono
            </div>

            <!-- /.col -->
        </div>
        <!-- /.row -->


        <div class="row">
            <!-- accepted payments column -->
            <div class="col-xs-12">
                <p class="lead">Detalle de Oparacion Cobranza</p>
                <div class="col-sm-12 invoice-col">
                    <b>Operacion ID:</b> @Model.operacion.operacionCobranzaID<br>
                    <b>Usuario:</b> @Model.operacion.UsrCre<br>
                    <b>Fecha Compromiso:</b> @Model.operacion.FechaCompromiso<br>
                    <b>Causal Anterior Mora:</b> @Model.operacion.CausalMoraAnt <br>
                    <b>Causal Mora:</b> @Model.operacion.NombreCausal <br>
                    <b>Compromido de Pago:</b> @Model.operacion.compromisoPago<br>
                    <b>Tipo de Gestion:</b> @Model.operacion.nombretipogetion<br>
                    <b>Presencia del Cliente:</b> @Model.operacion.nombrepresencia<br>
                    <b>Probalidad de Pago:</b> @Model.operacion.nombreprobalidadPago<br>
                    <b>Comentario:</b> @Model.operacion.Comentario
                </div>
            </div>
            <!-- /.col -->
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>
<!-- ./wrapper -->
